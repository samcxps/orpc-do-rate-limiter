{
  "name": "orpc-do-rate-limiter",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/orpc/orpc-do-rate-limiter.git"
  },
  "keywords": [
    "orpc"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    }
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "biome check .",
    "format": "biome format --write .",
    "type:check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cloudflare/workers-types": "^4.20251216.0",
    "@types/bun": "latest",
    "@types/node": "^24.10.2",
    "@vitest/coverage-v8": "4.0.15",
    "ultracite": "6.3.10",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/experimental-ratelimit": "^1.12.2"
  }
}
